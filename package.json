{
  "name": "n8n-nodes-neura-ianustec",
  "version": "0.1.0",
  "description": "n8n Community Node for NEURA | IANUSTEC AI - OpenAI compatible API integration with Chat Completions and Embeddings support",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "neura",
    "ianustec",
    "ai",
    "openai",
    "chat",
    "embeddings",
    "llm"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ianustec/n8n-nodes-neura-ianustec",
  "author": {
    "name": "IANUSTEC",
    "email": "info@ianustec.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ianustec/n8n-nodes-neura-ianustec.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials",
    "lintfix": "eslint nodes credentials --fix",
    "prepublishOnly": "npm run build && npm run lint && npm run format",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NeuraIanustecApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/NeuraIanustec/NeuraIanustec.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
